{
  "curly": true,
  "eqeqeq": true,
  "undef": true,
  "jquery": true,
  // global variables
  "globals": {
    "angular": false,
    "jasmine": false,
    "$": false,
    "_": false,
    "module": false,
    "require": false,
    "calendarApp": false,
    "moment": false,
    "CALENDAR_CONFIG": false
  },
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-multi-spaces": [2],
    "key-spacing": [2],
    "semi": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}
